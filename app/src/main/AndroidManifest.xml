<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eliabrian.fuzzyapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JurusanActivity" />
        <activity android:name=".InsertJurusan" />
        <activity android:name=".EditJurusan" />
        <activity android:name=".MahasiswaActivity" />
        <activity android:name=".InsertMahasiswa" />
        <activity android:name=".Beasiswa" />
        <activity android:name=".LaporanBeasiswa" />
        <activity android:name=".EditMahasiswa"></activity>
    </application>

</manifest>